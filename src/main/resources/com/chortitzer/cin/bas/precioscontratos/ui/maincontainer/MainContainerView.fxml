<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane id="masterVbox" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40"
			   fx:controller="com.chortitzer.cin.bas.precioscontratos.ui.maincontainer.MainContainerView">
	<!-- TODO Add Nodes -->
	<children>
		<TableView id="empresaTable" fx:id="empresaTable" AnchorPane.bottomAnchor="0.0"
				   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<columns>
				<TableColumn prefWidth="75.0" text="ID">
					<cellValueFactory>
						<PropertyValueFactory property="id"/>
					</cellValueFactory>
				</TableColumn>
				<TableColumn text="Nombre">
					<cellValueFactory>
						<PropertyValueFactory property="nombre"/>
					</cellValueFactory>
				</TableColumn>
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
			</columnResizePolicy>
		</TableView>
	</children>
</AnchorPane>
